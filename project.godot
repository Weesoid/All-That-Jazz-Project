;   param=value ; assign values to parameters

config_version=5

[application]

config/name="All That Jazz"
run/main_scene="res://scenes/user_interface/StartMenu.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/use_filter=false
config/icon="res://ATJ Icon.png"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
InputHelper="*res://addons/input_helper/input_helper.gd"
OverworldGlobals="*res://scripts/globals/gblOverworld.gd"
CombatGlobals="*res://scripts/globals/gblCombat.gd"
PlayerGlobals="*res://scripts/globals/gblPlayerData.gd"
InventoryGlobals="*res://scripts/globals/gblInventory.gd"
QuestGlobals="*res://scripts/globals/gblQuests.gd"
SaveLoadGlobals="*res://scripts/globals/gblSaveLoadManager.gd"
SettingsGlobals="*res://scripts/globals/gblSettings.gd"
ResourceGlobals="*res://scripts/globals/gblResourceManager.gd"
FootstepSoundManager="*res://scripts/globals/gblFootstepSoundManager.gd"

[dialogue_manager]

general/states=["Globals", "CombatGlobals", "OverworldGlobals", "PlayerGlobals", "InventoryGlobals", "QuestGlobals"]

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/resizable=false
window/size/window_width_override=1280
window/size/window_height_override=800
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://images/sprites/cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/custom="res://design/DefaultTheme.tres"
theme/custom_font="res://design/nokiafc22.ttf"
theme/default_font_antialiasing=0
timers/tooltip_delay_sec=0.05

