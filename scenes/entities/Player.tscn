[sub_resource type="GDScript" id="GDScript_lueap"]
script/source = "extends VBoxContainer

@onready var player = OverworldGlobals.getPlayer()

func _input(event):
	if player.is_processing_input() and player.power_inputs != '' and player.power_listening and (event is InputEventKey or event is InputEventJoypadButton):
		for power in await getValidPowers(player.power_inputs):
			add_child(createReference(power))
	
	if Input.is_action_just_released('ui_gambit'):
		await clearReferences()

func clearReferences():
	for child in get_children():
		child.queue_free()
		await child.tree_exited

func createReference(power: ResPower):
	var reference = preload(\"res://scenes/user_interface/PowerReference.tscn\").instantiate()
	reference.power = power
	return reference

func getValidPowers(inputs: String):
	await clearReferences()
	var valid_powers = []
	for power in PlayerGlobals.KNOWN_POWERS:
		if power.INPUT_MAP.begins_with(inputs): valid_powers.append(power)
	return valid_powers

"

