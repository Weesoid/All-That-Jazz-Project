[sub_resource type="GDScript" id="GDScript_ggh4h"]
script/source = "extends ProgressBar

func _process(_delta):
	value = getHPSum()
	max_value = getMaxHPSum()

func getHPSum():
	var out = 0
	for combatant in CombatGlobals.getCombatScene().getCombatantGroup('team'):
		if combatant.STAT_VALUES['health'] > 0: 
			out += combatant.STAT_VALUES['health']
	return out

func getMaxHPSum():
	var out = 0
	for combatant in CombatGlobals.getCombatScene().getCombatantGroup('team'):
		out += combatant.getMaxHealth()
	return out
"

