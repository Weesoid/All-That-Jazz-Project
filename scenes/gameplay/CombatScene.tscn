[sub_resource type="GDScript" id="GDScript_ip03s"]
resource_name = "TensionBar"
script/source = "extends ProgressBar

@onready var combat_scene = CombatGlobals.getCombatScene()
@onready var animator = $AnimationPlayer
@onready var bar_fader = $FaderBar
var previous_value

func _ready():
	CombatGlobals.active_combatant_changed.connect(_on_active_combatant_changed)
	CombatGlobals.tension_changed.connect(updateValue)
	show()
	animator.play(\"Show\")
	updateValue(0, CombatGlobals.TENSION)

func _on_active_combatant_changed(combatant):
	if combatant is ResPlayerCombatant and !visible:
		show()
		animator.play(\"Show\")
	elif combatant is ResEnemyCombatant and !animator.is_playing():
		hide()
		animator.play_backwards(\"Show\")

#func _process(_delta):
#	value = CombatGlobals.TENSION

func _on_value_changed(_val):
	if !combat_scene.active_combatant is ResPlayerCombatant and !animator.is_playing() and value != 0:
		show()
		animator.play(\"Show\")

func updateValue(prev_tension, tension):
	animateFaderBar(prev_tension, tension)
	value = tension

func animateFaderBar(prev_val, val):
#	print(prev_val, ' vs ', val)
	if prev_val == val:
		return
	
	var tween = create_tween().set_trans(Tween.TRANS_CUBIC)
	if prev_val > val:
		tween.tween_property(bar_fader, 'modulate', Color.RED, 0.0)
	elif prev_val < val:
		tween.tween_property(bar_fader, 'modulate', Color.GREEN, 0.0)
	tween.tween_method(setFaderBarValue, prev_val, val, 0.15)
	tween.tween_property(bar_fader, 'modulate', Color.TRANSPARENT, 0.25)
	#tween.set_parallel(true)

func setFaderBarValue(val):
	bar_fader.value = val
"

