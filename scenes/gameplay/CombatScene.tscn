resource_name = "TensionBar"
script/source = "extends CustomCountBar

@onready var combat_scene = CombatGlobals.getCombatScene()
@onready var animator = $AnimationPlayer

var previous_value
var bar_filling:bool = false

func _ready():
	CombatGlobals.active_combatant_changed.connect(_on_active_combatant_changed)
	CombatGlobals.tension_changed.connect(updateValue)
	#show()
	#animator.play(\"Show\")
	#await get_tree().process_frame
	updateValue(0, CombatGlobals.tension)

func _on_active_combatant_changed(combatant):
	if combatant is ResPlayerCombatant and !visible and combatant.ai_package == null:
		show()
		animator.play(\"Show\")
	elif combatant is ResEnemyCombatant and !animator.is_playing():
		hide()
		animator.play_backwards(\"Show\")

#func _process(_delta):
#	value = CombatGlobals.TENSION

#func _on_value_changed(_val):
#	if (!combat_scene.active_combatant is ResPlayerCombatant or (combat_scene.active_combatant is ResPlayerCombatant and !visible)) and !animator.is_playing() and value != 0:
#		show()
#		animator.play(\"Show\")

func updateValue(_prev_tension, tension):
	value = tension
	if (!combat_scene.active_combatant is ResPlayerCombatant or (combat_scene.active_combatant is ResPlayerCombatant and !visible)) and !animator.is_playing() and value != 0:
		show()
		animator.play(\"Show\")
"

